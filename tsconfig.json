{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["node", "react"],
    "baseUrl": "."
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "raycast-env.d.ts"]
}
